    "@fastify/session": "^10.5.0",
    "@fastify/static": "^6.10.2",
    "@fastify/swagger": "^8.12.1",
    "@fastify/swagger-ui": "^2.0.1",
    "@ffmpeg.wasm/core-mt": "^0.12.0",
